<h1>Home#index</h1>
<p>Find me in app/views/home/index.html.erb</p>

<% if current_user.blank? %>
    <%= button_to "sign in", new_user_session_path  %>
<% end %>


<ul>
    <% @posts.each do |post| %>
        <li>
            <h5><%= post.title %></h5>
            <%= link_to "edit", edit_post_path(post) %>
        </li>
    <% end %>
</ul>
